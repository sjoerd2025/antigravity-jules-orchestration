# Jules MCP Server Configuration
# Copy this file to .env and fill in your values

# ===================
# Required Settings
# ===================

# Jules API Key (required for Jules API calls)
# Get your API key from: https://jules.google.com
JULES_API_KEY=your_jules_api_key_here

# ===================
# Optional Settings
# ===================

# Server Configuration
PORT=3323
HOST=127.0.0.1

# GitHub Token (for GitHub API integrations)
# Create at: https://github.com/settings/tokens
GITHUB_TOKEN=ghp_your_github_token_here

# Database URL (PostgreSQL for orchestrator-api)
# Format: postgresql://user:password@host:port/database
DATABASE_URL=postgresql://user:password@localhost:5432/jules_orchestrator

# Slack Webhook (for notifications)
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/xxx/xxx/xxx

# Semantic Memory MCP Server URL
SEMANTIC_MEMORY_URL=http://localhost:3324

# Logging
LOG_LEVEL=info

# CORS Configuration (comma-separated origins)
# Leave empty to use defaults: localhost:3000, localhost:5173, antigravity-jules-orchestration.onrender.com
ALLOWED_ORIGINS=

# ===================
# Feature Flags
# ===================

# Enable response compression (gzip)
COMPRESSION_ENABLED=true

# Enable response caching
CACHE_ENABLED=false

# Redis URL (required if CACHE_ENABLED=true)
REDIS_URL=redis://localhost:6379

# ===================
# Render Integration (Optional)
# ===================

# Render API Key (starts with rnd_)
# Get from: https://dashboard.render.com/u/settings/api-keys
RENDER_API_KEY=rnd_your_render_api_key_here

# Render Webhook Secret (for signature verification)
RENDER_WEBHOOK_SECRET=your_webhook_secret_here
